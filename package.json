	"license": "GPL-3.0",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"sass": "^1.68.0",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
