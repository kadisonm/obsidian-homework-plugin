{
	"name": "obsidian-homework-manager",
	"version": "1.1.0",
	"description": "Keeps track of homework through a to-do list.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
